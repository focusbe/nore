{
    "name": "webcreator",
    "version": "0.0.1",
    "description": "集成前端开发，切图，逻辑，组件调用，项目发布的客户端",
    "productFileName": "webcreator",
    "main": "dist/main/index.js",
    "scripts": {
        "start-main": "webpack --config build/main.config.js --progress --hide-modules && npm run electron ./dist/main/index.js",
        "electron": "electron ./dist/main/index.js",
        "dev": "webpack-dev-server --progress --colors --content-base build --config build/renderer.config.js",
        "dist-win": "set NODE_ENV=production&& npm run build",
        "dist-mac": "export NODE_ENV=production&& npm run build",
        "build": "webpack --config build/renderer.config.js --progress --hide-modules",
        "start": "npm run dev && npm run electron",
        "rebuild": "electron-rebuild -f -w ",
        "packwin": "npm run dist && electron-builder --x64 --publish always",
        "packmac": "npm run dist-mac && electron-builder --publish always"
    },
    "build": {
        "appId": "com.ztgame.webcreator",
        "asar": true,
        "publish": [
            {
                "provider": "github",
                "repo": "webcreator",
                "owner": "foliouTeam",
                "protocol": "https",
                "token": "9d6f0554d880238a35b71a40c9bb129618070ff6"
            }
        ],
        "asarUnpack": [
            "./template/**/*",
            "./config/**/*",
            "./data/**/*"
        ],
        "mac": {
            "category": "your.app.category.type",
            "icon": "assets/app",
            "target": [
                "zip",
                "dmg"
            ]
        },
        "win": {
            "target": "nsis",
            "icon": "assets/app"
        },
        "squirrelWindows": {
            "iconUrl": "http://www.ztgame.com/logo.png"
        },
        "files": {
            "filter": [
                "**/*",
                "!**/node_modules/*/{CHANGELOG.md,README.md,README,readme.md,readme,test,__tests__,tests,powered-test,example,examples,*.d.ts}",
                "!**/node_modules/.bin",
                "!**/*.{iml,o,hprof,orig,pyc,pyo,rbc,swp,csproj,sln,xproj}",
                "!**/._*",
                "!.editorconfig",
                "!package.json",
                "!README.md",
                "!tsconfig.json",
                "!yarn.lock",
                "!webpack.config.js",
                "!package-lock.json",
                "!app/*",
                "!output/*",
                "!assets/*",
                "!build/*",
                "!workshop/*"
            ]
        },
        "directories": {
            "output": "output"
        }
    },
    "author": "刘鹏",
    "license": "",
    "devDependencies": {
        "@babel/core": "^7.2.2",
        "babel-loader": "^8.0.4",
        "babel-plugin-import": "^1.6.7",
        "babel-preset-env": "^1.7.0",
        "babel-preset-stage-2": "^6.13.0",
        "clipboard": "^1.7.1",
        "css-loader": "^2.1.0",
        "electron": "^4.0.0",
        "electron-updater": "^4.0.6",
        "element-ui": "^2.4.11",
        "file-loader": "^3.0.1",
        "html-loader": "^0.5.5",
        "html-webpack-plugin": "^3.2.0",
        "image-webpack-loader": "^4.6.0",
        "iview": "^3.2.1",
        "iview-loader": "^1.2.2",
        "jquery": "^3.2.1",
        "json-loader": "^0.5.7",
        "less": "^2.7.3",
        "less-loader": "^4.1.0",
        "material-design-icons": "^3.0.1",
        "node-sass": "^4.11.0",
        "sass-loader": "^7.1.0",
        "style-loader": "^0.23.1",
        "ts-loader": "^5.3.2",
        "ts-node": "^7.0.1",
        "typescript": "^3.2.2",
        "url-loader": "^1.1.2",
        "vue": "^2.5.21",
        "vue-loader": "^15.4.2",
        "vue-router": "^3.0.2",
        "vue-server-renderer": "^2.5.21",
        "vue-style-loader": "^4.1.2",
        "vue-template-compiler": "^2.5.21",
        "vuex": "^3.0.1",
        "webpack": "^4.28.3",
        "webpack-cli": "^3.1.2",
        "webpack-dev-server": "^3.1.14"
    },
    "dependencies": {}
}
